
<div class="row">
    <% categoryDatas.forEach((category)=> { %>
      <div class="col-sm-6 col-md-6 col-lg-4 mb-4 mb-lg-0" data-aos="fade" data-aos-delay="">
        <a class="block-2-item" href="/shop">
          <figure class="image">
            <img src="<%= category.image %>" alt="" class="img-fluid">
          </figure>
          <div class="text">
            <h3>
              <%= category.name %>
            </h3>
          </div>
        </a>
      </div>
      <% }) %>
  </div>